{
  "name": "tilejson.io",
  "version": "0.0.0",
  "description": "",
  "private": true,
  "main": "main.js",
  "scripts": {
    "bundle": "rm -rf /usr/dist/* && webpack --config webpack.prod.config.js --colors && echo $VERSION > /usr/dist/version.txt",
    "dev-server": "node_modules/.bin/webpack-dev-server --config webpack.dev.config.js --inline --hot --progress --colors --host 0.0.0.0 --port 4567 --history-api-fallback",
    "test": "jest --colors",
    "postinstall": "npm rebuild node-sass"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azavea/tilejson.io.git"
  },
  "author": "Azavea",
  "bugs": {
    "url": "https://github.com/azavea/tilejson.io/issues"
  },
  "homepage": "https://github.com/azavea/tilejson.io",
  "jest": {
    "moduleFileExtensions": ["js", "jsx"],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/reactTests/__mocks__/fileMock.js",
      "\\.(css|sass|less)$": "<rootDir>/reactTests/__mocks__/styleMock.js",
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/legacy_notes_and_workbook/",
    ]
  },
